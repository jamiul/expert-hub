{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "anandsiddharth/laravel-paytm-wallet": "^2.0",
        "aws/aws-sdk-php": "^3.281",
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.23",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "kingflamez/laravelrave": "^4.2",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.9",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "league/flysystem-aws-s3-v3": "^3.0",
        "mehedi-iitdu/core-component-repository": "^2.2",
        "paypal/paypal-checkout-sdk": "^1.0",
        "phpoffice/phpword": "^1.1",
        "smalot/pdfparser": "^2.7",
        "spatie/laravel-permission": "^5.11",
        "spatie/pdf-to-text": "^1.52",
        "stripe/stripe-php": "^12.3",
        "tecnickcom/tcpdf": "^6.6",
        "unicodeveloper/laravel-paystack": "^1.1",
        "yajra/laravel-datatables-oracle": "^10.8"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "beyondcode/laravel-dump-server": "^1.9",
        "filp/whoops": "^2.15",
        "kitloong/laravel-migrations-generator": "^6.10",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^7.0",
        "orangehill/iseed": "^3.0",
        "phpunit/phpunit": "^10.3",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "PhpOffice\\PhpWord\\": "vendor/phpoffice/phpword/src/PhpWord"
        },
        "files": ["app/Http/Helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },


    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
